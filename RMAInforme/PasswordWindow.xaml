<Window x:Class="RMAInforme.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RMAInforme"
        mc:Ignorable="d"
        Title="Cambio de estado" Height="170" Width="280" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,2,2">
        <Button Name="OKButton" Content="CAMBIAR" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="72" Click="OKButton_Click" FontFamily="Consolas" Height="24"/>
        <PasswordBox Name="Pass" Height="23" Margin="0,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="150" VerticalContentAlignment="Center"/>
        <TextBox Name="ObsText" MaxLength="30" HorizontalAlignment="Right" Height="50" Margin="0,40,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150" FontFamily="Consolas" FontSize="14"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontFamily="Consolas" Width="100"/>
        <Label Content="Observaciones" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" FontFamily="Consolas" Width="100"/>

    </Grid>
</Window>
